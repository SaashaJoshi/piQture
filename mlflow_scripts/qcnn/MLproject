name: MLflow Workflow for QCNN

conda_env: conda_env.yaml

entry_points:
    # Data Processing Stage
    load_mnist_data:
        parameters:
            labels: {default: None}
            batch_size: {type: int, default: None}
            img_size: {default: None}
            norm_min: {default: None}
            norm_max: {default: None}
        command: "python data_loading.py --labels {labels} --batch_size {batch_size} --img_size {img_size} --norm_min {norm_min} --norm_max {norm_max}"
